<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <!--电压-->
    <servlet>
        <servlet-name>currentServlet</servlet-name>
        <servlet-class>currentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>currentServlet</servlet-name>
        <url-pattern>/currentServlet</url-pattern>
    </servlet-mapping>
    <!--温度 -->
    <servlet>
        <servlet-name>temperatureServlet</servlet-name>
        <servlet-class>temperatureServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>temperatureServlet</servlet-name>
        <url-pattern>/temperatureServlet</url-pattern>
    </servlet-mapping>
    <!--按钮状态 -->
    <servlet>
        <servlet-name>ButtonStatusServlet</servlet-name>
        <servlet-class>ButtonStatusServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ButtonStatusServlet</servlet-name>
        <url-pattern>/ButtonStatusServlet</url-pattern>
    </servlet-mapping>
    <!--实时数据 -->
    <servlet>
        <servlet-name>DataServlet</servlet-name>
        <servlet-class>DataServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DataServlet</servlet-name>
        <url-pattern>/DataServlet</url-pattern>
    </servlet-mapping>
</web-app>